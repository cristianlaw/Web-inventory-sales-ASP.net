@model MisMinistritosUNED.Models.ProductoModel

@{
    ViewData["Title"] = "Detalles del Producto";
}

<p style="color:darkorange" ;>@TempData["prodactualizado"]</p>

<h1>Detalles del Producto</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            <label class="control-label">Id</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.id)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Descripción:</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descripcion)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Fecha de ingreso:</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ingreso)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Precio de compra:</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.precioCompra)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Utilidad:</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.utilidad)
        </dd>
        <dt class="col-sm-2">
            <label class="control-label">Proveedor:</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.proveedor)
        </dd>
    </dl>
</div>
<hr />
<div>
    <a asp-action="List">Volver a la lista</a>
</div>
<script type="text/javascript">
    function preventBack() { window.history.forward(); }
    setTimeout("preventBack()", 0);
    window.onunload = function () { null };
</script>
