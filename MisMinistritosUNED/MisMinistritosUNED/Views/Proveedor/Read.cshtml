@model MisMinistritosUNED.Models.ProveedorModel

@{
    ViewData["Title"] = "Detalles del Proveedor";
}

<p style="color:darkorange" ;>@TempData["provactualizado"]</p>


<h1>Detalles del Proveedor</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label class="control-label">Cédula</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.cedula)
        </dd>
        <dt class="col-sm-2">
            <label class="control-label">Descripción</label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.descripcion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="List">Volver a la lista</a>
</div>
<script type="text/javascript">
    function preventBack() { window.history.forward(); }
    setTimeout("preventBack()", 0);
    window.onunload = function () { null };
</script>
